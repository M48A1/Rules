#!name=Google搜索重定向
#!desc=将Google搜索重定向至.com域名，如遇频繁跳转请禁用本插件。

#!category=Enchance

#!homepage=https://github.com/luestr/ProxyResource/blob/main/README.md


[URL Rewrite]
^https?:\/\/(www.)?(g|google)\.cn https://www.google.com 307
^https?:\/\/(ditu|maps).google\.cn https://maps.google.com 307
^https?:\/\/(www.)?(g|google)\.com.hk https://www.google.com 307

[MITM]
hostname = %APPEND% www.google.cn
hostname = %APPEND% www.google.com.hk
